<mvc:View
   xmlns:mvc="sap.ui.core.mvc"
   xmlns="sap.m"
   xmlns:core="sap.ui.core"
   controllerName="comboboxnorthwind.controller.View1">
   <App id="_IDGenApp1">
   <Page id="_IDGenPage1" >
   <VBox id="_IDGenVBox1">
      <ComboBox xmlns="sap.m" id="comboBox" items="{/Customers}">
      <core:Item key="{CustomerID}" text="{CompanyName}"/>
      </ComboBox>

      <List id="list" items="{/Products}">
         <StandardListItem id="_IDGenStandardListItem1" title="{ProductName}" />
      </List>
   </VBox>
   </Page>
   </App>
</mvc:View>